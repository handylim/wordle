{
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"**/*.d.ts"
	],
	"exclude": [
		"node_modules"
	]
}